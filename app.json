{
  "expo": {
    "name": "Native Starter kit",
    "slug": "native-startkit",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./source/assets/images/head.png",
    "splash": {
      "image": "./source/assets/images/head.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0FAFA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "source/assets/**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
